	"type": "module",
	"scripts": {
		"format": "prettier --write .",
		"lint": "prettier --write . && eslint . --fix",
		"typecheck": "tsc --noEmit --watch",
		"dev": "vinxi dev",
		"build": "vinxi build",
		"start": "vinxi start",
		"db:backup": "bash src/server/db/scripts/backup.sh",
		"db:backup-remote": "bash src/server/db/scripts/backup-remote.sh",
		"db:restore": "bash src/server/db/scripts/restore.sh",
		"db:restore-remote": "bash src/server/db/scripts/restore-remote.sh",
		"db:migrate": "drizzle-kit migrate",
		"db:generate": "drizzle-kit generate",
		"db:studio": "drizzle-kit studio",
		"sync:daily": "bun run sync:arc && bun run sync:raindrop && bun run sync:readwise && bun run sync:github",
		"sync:adobe": "bun run src/server/integrations/adobe/sync.ts",
		"sync:airtable": "bun run src/server/integrations/airtable/sync.ts",
		"sync:arc": "bun run src/server/integrations/arc/sync.ts",
		"sync:github": "bun run src/server/integrations/github/sync.ts",
		"sync:raindrop": "bun run src/server/integrations/raindrop/sync.ts",
		"sync:readwise": "bun run src/server/integrations/readwise/sync.ts",
		"sync:twitter": "bun run src/server/integrations/twitter/sync.ts"
	},
