	"author": {
		"name": "Nick Trombley",
		"email": "trombley.nick@gmail.com",
		"url": "https://nicktrombley.design"
	},
	"type": "module",
	"scripts": {
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"check": "tsc --noEmit",
		"check:watch": "tsc --noEmit --watch",
		"dev": "vinxi dev",
		"build": "vinxi build",
		"start": "vinxi start",
		"db:backup": "bash scripts/backup.sh",
		"db:restore": "bash scripts/restore.sh",
		"db:migrate": "drizzle-kit migrate",
		"db:generate": "drizzle-kit generate",
		"db:studio": "drizzle-kit studio",
		"sync:daily": "bun run sync:arc && bun run sync:raindrop && bun run sync:readwise && bun run sync:github",
		"sync:adobe": "bun run db/schema/integrations/adobe/sync.ts",
		"sync:airtable": "bun run db/schema/integrations/airtable/sync.ts",
		"sync:arc": "bun run db/schema/integrations/arc/sync.ts",
		"sync:github": "bun run db/schema/integrations/github/sync.ts",
		"sync:raindrop": "bun run db/schema/integrations/raindrop/sync.ts",
		"sync:readwise": "bun run db/schema/integrations/readwise/sync.ts",
		"sync:twitter": "bun run db/schema/integrations/twitter/sync.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.717.0",
		"@libsql/client": "^0.14.0",
		"@octokit/request-error": "^6.1.6",
