		"@tanstack/react-router": "^1.93.0",
		"@tanstack/react-table": "^8.20.6",
		"@tanstack/start": "^1.93.0",
		"airtable": "^0.12.2",
		"classnames": "^2.5.1",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.38.3",
		"drizzle-zod": "^0.6.1",
		"mime-types": "^2.1.35",
		"next-themes": "^0.4.4",
		"openai": "^4.77.0",
		"pg": "^8.13.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"vinxi": "^0.5.1",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@octokit/types": "^13.6.2",
		"@types/bun": "^1.1.14",
