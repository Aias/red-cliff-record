		"@tanstack/react-query": "^5.64.1",
		"@tanstack/react-router": "^1.95.5",
		"@tanstack/react-router-with-query": "^1.95.5",
		"@tanstack/react-table": "^8.20.6",
		"@tanstack/start": "^1.95.5",
		"@trpc/client": "^11.0.0-rc.446",
