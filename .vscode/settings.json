{
	"eslint.useFlatConfig": true,
	"eslint.workingDirectories": ["."],
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"files.associations": {
		"*.css": "tailwindcss",
		"*.svg": "html"
	},
	"files.watcherExclude": {
		"**/node_modules/**": true,
		"**/dist/**": true,
		"**/.vinxi/**": true,
		"**/.output/**": true
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/*.code-search": true,
		"**/dist": true,
		"**/.vinxi": true,
		"**/.output": true
	},
	"typescript.tsserver.maxTsServerMemory": 8000,
	"typescript.tsserver.nodePath": "node"
}
